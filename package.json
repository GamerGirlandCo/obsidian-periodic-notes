{
  "name": "obsidian-periodic-notes",
  "version": "0.0.17",
  "description": "Create/manage daily, weekly, and monthly notes",
  "author": "liamcain",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "lint": "svelte-check && eslint . --ext .ts && tsc --noEmit",
    "lint:fix": "svelte-check && eslint . --ext .ts --fix",
    "dev": "node --experimental-json-modules esbuild.config.mjs",
    "build": "yarn run lint && node --experimental-json-modules esbuild.config.mjs production",
    "release": "standard-version",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test -- --watch"
  },
  "standard-version": {
    "t": ""
  },
  "dependencies": {
    "@popperjs/core": "2.11.5",
    "lodash": "4.17.21",
    "obsidian": "0.14.4",
    "svelte": "3.47.0",
    "svelte-writable-derived": "^2.1.3",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@tsconfig/svelte": "3.0.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.181",
    "@types/moment": "2.13.0",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "builtin-modules": "^3.2.0",
    "dotenv": "16.0.0",
    "esbuild": "0.14.34",
    "esbuild-svelte": "0.7.0",
    "eslint": "8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "26.6.3",
    "moment": "2.29.2",
    "sass": "^1.50.0",
    "standard-version": "^9.3.2",
    "svelte-check": "2.6.0",
    "svelte-preprocess": "4.10.5",
    "ts-jest": "26.5.3",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
}
